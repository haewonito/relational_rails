<!-- ''/artists' : shows all of artist names -->

<style>
  h6 {text-align:right; margin:0;}
</style>

<h6><a href="/artists">Artist Names</a></h6>
<h6><a href="/records">All Records</a></h6>
<h6><a href="/artists/new">New Artist</a></h6>



<h1>Artists</h1>
<% @artists.each do |artist| %>
  <p>
    <%= artist.first_name + " " + artist.name %>
    <%= link_to("Edit", "/artists/#{artist.id}/edit")%>
    <br>
    <%= "Created At: " %>
    <%= artist.created_at %>

  </p>
<% end %>
